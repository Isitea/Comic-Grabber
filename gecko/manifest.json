{
	"manifest_version": 2,
	"name": "Comic grabber 2",
    "version": "2.0.9",
    "default_locale": "ko",
	"description": "__MSG_extensionDescription__",
	"author": "Isitea",
	"icons": {
		"128": "icon-128.png"
	},
	"permissions": [
		"downloads",
		"<all_urls>"
	],
	"content_scripts": [
		{
			"matches": [
                "http://*/*",
                "https://*/*"
			],
			"js": [
				"app/loader.js"
			],
			"run_at": "document_end",
			"all_frames": false
		}
    ],
    "web_accessible_resources": [ "*" ],
    "background": {
        "scripts": [ "services/serviceProvider.js" ]
	},
	"browser_specific_settings": {
		"gecko": {
			"strict_min_version": "70.0"
		}
	}
}
