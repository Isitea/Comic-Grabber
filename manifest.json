{
	"manifest_version": 2,
	"name": "Tistory downloader",
	"version": "0.0.1",
	"description": "This extension downloads attachments in page automatically.",
	"author": "Isitea",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"downloads"
	],
	"content_scripts": [
		{
			"matches": [
				"*://*.tistory.com/*"
			],
			"js": [
				"scanner.js"
			],
			"run_at": "document_end",
			"all_frames": false
		}
	],
	"background": {
		"page": "watcher.html"
	},
	"minimum_chrome_version": "61"
}